@article{lam_genetic_2018,
	title = {Genetic diversity, mobilisation and spread of the yersiniabactin-encoding mobile element {ICEKp} in {Klebsiella} pneumoniae populations},
	volume = {4},
	issn = {,},
	url = {https://www.microbiologyresearch.org/content/journal/mgen/10.1099/mgen.0.000196},
	doi = {10.1099/mgen.0.000196},
	abstract = {Mobile genetic elements (MGEs) that frequently transfer within and between bacterial species play a critical role in bacterial evolution, and often carry key accessory genes that associate with a bacteria’s ability to cause disease. MGEs carrying antimicrobial resistance (AMR) and/or virulence determinants are common in the opportunistic pathogen {\textless}em class=""{\textgreater}Klebsiella pneumoniae{\textless}/em{\textgreater}, which is a leading cause of highly drug-resistant infections in hospitals. Well-characterised virulence determinants in {\textless}em class=""{\textgreater}K. pneumoniae{\textless}/em{\textgreater} include the polyketide synthesis loci {\textless}em class=""{\textgreater}ybt{\textless}/em{\textgreater} and {\textless}em class=""{\textgreater}clb{\textless}/em{\textgreater} (also known as {\textless}em class=""{\textgreater}pks{\textless}/em{\textgreater}), encoding the iron-scavenging siderophore yersiniabactin and genotoxin colibactin, respectively. These loci are located within an MGE called ICE{\textless}em class=""{\textgreater}Kp{\textless}/em{\textgreater}, which is the most common virulence-associated MGE of {\textless}em class=""{\textgreater}K. pneumoniae,{\textless}/em{\textgreater} providing a mechanism for these virulence factors to spread within the population. Here we apply population genomics to investigate the prevalence, evolution and mobility of {\textless}em class=""{\textgreater}ybt{\textless}/em{\textgreater} and {\textless}em class=""{\textgreater}clb{\textless}/em{\textgreater} in {\textless}em class=""{\textgreater}K. pneumoniae{\textless}/em{\textgreater} populations through comparative analysis of 2498 whole-genome sequences. The {\textless}em class=""{\textgreater}ybt{\textless}/em{\textgreater} locus was detected in 40 \% of {\textless}em class=""{\textgreater}K. pneumoniae{\textless}/em{\textgreater} genomes, particularly amongst those associated with invasive infections. We identified 17 distinct {\textless}em class=""{\textgreater}ybt{\textless}/em{\textgreater} lineages and 3 {\textless}em class=""{\textgreater}clb{\textless}/em{\textgreater} lineages, each associated with one of 14 different structural variants of ICE{\textless}em class=""{\textgreater}Kp{\textless}/em{\textgreater}. Comparison with the wider population of the family {\textless}em class=""{\textgreater}Enterobacteriaceae{\textless}/em{\textgreater} revealed occasional ICE{\textless}em class=""{\textgreater}Kp{\textless}/em{\textgreater} acquisition by other members. The {\textless}em class=""{\textgreater}clb{\textless}/em{\textgreater} locus was present in 14 \% of all {\textless}em class=""{\textgreater}K. pneumoniae{\textless}/em{\textgreater} and 38.4 \% of {\textless}em class=""{\textgreater}ybt{\textless}/em{\textgreater}+ genomes. Hundreds of independent ICE{\textless}em class=""{\textgreater}Kp{\textless}/em{\textgreater} integration events were detected affecting hundreds of phylogenetically distinct {\textless}em class=""{\textgreater}K. pneumoniae{\textless}/em{\textgreater} lineages, including at least 19 in the globally-disseminated carbapenem-resistant clone CG258. A novel plasmid-encoded form of {\textless}em class=""{\textgreater}ybt{\textless}/em{\textgreater} was also identified, representing a new mechanism for {\textless}em class=""{\textgreater}ybt{\textless}/em{\textgreater} dispersal in {\textless}em class=""{\textgreater}K. pneumoniae{\textless}/em{\textgreater} populations. These data indicate that MGEs carrying {\textless}em class=""{\textgreater}ybt{\textless}/em{\textgreater} and {\textless}em class=""{\textgreater}clb{\textless}/em{\textgreater} circulate freely in the {\textless}em class=""{\textgreater}K. pneumoniae{\textless}/em{\textgreater} population, including among multidrug-resistant strains, and should be considered a target for genomic surveillance along with AMR determinants.,},
	number = {9},
	urldate = {2020-07-20},
	journal = {Microbial Genomics,},
	author = {Lam, Margaret M. C. and Wick, Ryan R. and Wyres, Kelly L. and Gorrie, Claire L. and Judd, Louise M. and Jenney, Adam W. J. and Brisse, Sylvain and Holt, Kathryn E.},
	year = {2018},
	note = {Publisher: Microbiology Society,},
	pages = {e000196},
}

