@article{ndao_diagnosis_2009,
	title = {Diagnosis of {Parasitic} {Diseases}: {Old} and {New} {Approaches}},
	volume = {2009},
	issn = {1687-708X},
	shorttitle = {Diagnosis of {Parasitic} {Diseases}},
