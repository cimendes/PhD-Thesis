	note = {Number: 15
Publisher: Multidisciplinary Digital Publishing Institute},
	keywords = {immunochromatographic strip test, lateral flow assay applications, lateral flow immunoassay, paper-based biosensor, point-of-care testing, rapid diagnostic test},
	pages = {5185},
}

