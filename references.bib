@article{al-shomrani_genomic_2020,
	title = {Genomic {Sequencing} and {Analysis} of {Eight} {Camel}-{Derived} {Middle} {East} {Respiratory} {Syndrome} {Coronavirus} ({MERS}-{CoV}) {Isolates} in {Saudi} {Arabia}},
	volume = {12},
