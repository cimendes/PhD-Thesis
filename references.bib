	journal = {Diagnostics},
	author = {Muhamad Rizal, Nurnabila Syafiqah and Neoh, Hui-min and Ramli, Ramliza and A/L K Periyasamy, Petrick @ Ramesh and Hanafiah, Alfizah and Abdul Samat, Muttaqillah Najihan and Tan, Toh Leong and Wong, Kon Ken and Nathan, Sheila and Chieng, Sylvia and Saw, Seow Hoon and Khor, Bee Yin},
	month = oct,
	year = {2020},
	note = {Number: 10
Publisher: Multidisciplinary Digital Publishing Institute},
	keywords = {16S rRNA next-generation sequencing, bacterial pathogen identification, culture and biochemical testing, diagnostic microbiology, middle-income countries},
	pages = {816},
}

