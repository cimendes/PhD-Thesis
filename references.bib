	urldate = {2022-01-31},
	journal = {Journal of Clinical Virology},
	author = {López-Labrador, F. Xavier and Brown, Julianne R. and Fischer, Nicole and Harvala, Heli and Van Boheemen, Sander and Cinek, Ondrej and Sayiner, Arzu and Madsen, Tina Vasehus and Auvinen, Eeva and Kufner, Verena and Huber, Michael and Rodriguez, Christophe and Jonges, Marcel and Hönemann, Mario and Susi, Petri and Sousa, Hugo and Klapper, Paul E. and Pérez-Cataluňa, Alba and Hernandez, Marta and Molenkamp, Richard and der Hoek, Lia van and Schuurman, Rob and Couto, Natacha and Leuzinger, Karoline and Simmonds, Peter and Beer, Martin and Höper, Dirk and Kamminga, Sergio and Feltkamp, Mariet C. W. and Rodríguez-Díaz, Jesús and Keyaerts, Els and Nielsen, Xiaohui Chen and Puchhammer-Stöckl, Elisabeth and Kroes, Aloys C. M. and Buesa, Javier and Breuer, Judy and Claas, Eric C. J. and de Vries, Jutte J. C.},
