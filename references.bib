	pmcid = {PMC3352080},
	keywords = {Case-Control Studies, Cluster Analysis, Endemic Diseases, Female, Genotype, Humans, Longitudinal Studies, Male, Nepal, Phylogeny, Polymorphism, Single Nucleotide, Salmonella paratyphi A, Salmonella typhi, Salmonella, typhoid, Paratyphoid, genotyping, transmission, geospatial, Typhoid Fever, Urban Population, Water Microbiology, Water Supply},
