}

@article{phelan_integrating_2019,
	title = {Integrating informatics tools and portable sequencing technology for rapid detection of resistance to anti-tuberculous drugs},
	volume = {11},
	issn = {1756-994X},
	url = {https://doi.org/10.1186/s13073-019-0650-x},
	doi = {10.1186/s13073-019-0650-x},
	abstract = {Mycobacterium tuberculosis resistance to anti-tuberculosis drugs is a major threat to global public health. Whole genome sequencing (WGS) is rapidly gaining traction as a diagnostic tool for clinical tuberculosis settings. To support this informatically, previous work led to the development of the widely used TBProfiler webtool, which predicts resistance to 14 drugs from WGS data. However, for accurate and rapid high throughput of samples in clinical or epidemiological settings, there is a need for a stand-alone tool and the ability to analyse data across multiple WGS platforms, including Oxford Nanopore MinION.},
