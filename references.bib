	pmcid = {PMC3651993},
	keywords = {Cohort Studies, Databases, Factual, Dengue, Dengue Virus, Global Health, Humans, Incidence, Public Health, Quality Control, Rain, Risk Factors, Temperature, Tropical Climate, Urbanization, World Health Organization},
