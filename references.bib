}

@article{du_plessis_establishment_2021,
	title = {Establishment and lineage dynamics of the {SARS}-{CoV}-2 epidemic in the {UK}},
	volume = {371},
	issn = {1095-9203},
	doi = {10.1126/science.abf2946},
	abstract = {The United Kingdom's COVID-19 epidemic during early 2020 was one of world's largest and was unusually well represented by virus genomic sampling. We determined the fine-scale genetic lineage structure of this epidemic through analysis of 50,887 severe acute respiratory syndrome coronavirus 2 (SARS-CoV-2) genomes, including 26,181 from the UK sampled throughout the country's first wave of infection. Using large-scale phylogenetic analyses combined with epidemiological and travel data, we quantified the size, spatiotemporal origins, and persistence of genetically distinct UK transmission lineages. Rapid fluctuations in virus importation rates resulted in {\textgreater}1000 lineages; those introduced prior to national lockdown tended to be larger and more dispersed. Lineage importation and regional lineage diversity declined after lockdown, whereas lineage elimination was size-dependent. We discuss the implications of our genetic perspective on transmission dynamics for COVID-19 epidemiology and control.},
	language = {eng},
	number = {6530},
	journal = {Science (New York, N.Y.)},
	author = {du Plessis, Louis and McCrone, John T. and Zarebski, Alexander E. and Hill, Verity and Ruis, Christopher and Gutierrez, Bernardo and Raghwani, Jayna and Ashworth, Jordan and Colquhoun, Rachel and Connor, Thomas R. and Faria, Nuno R. and Jackson, Ben and Loman, Nicholas J. and O'Toole, Áine and Nicholls, Samuel M. and Parag, Kris V. and Scher, Emily and Vasylyeva, Tetyana I. and Volz, Erik M. and Watts, Alexander and Bogoch, Isaac I. and Khan, Kamran and {COVID-19 Genomics UK (COG-UK) Consortium} and Aanensen, David M. and Kraemer, Moritz U. G. and Rambaut, Andrew and Pybus, Oliver G.},
	month = feb,
