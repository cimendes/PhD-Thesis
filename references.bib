	keywords = {Adult, Australia, Betacoronavirus, COVID-19, Coronavirus Infections, Female, Genome, Viral, Genomics, Health Personnel, Humans, Male, Middle Aged, Molecular Epidemiology, Pandemics, Phylogeny, Pneumonia, Viral, Public Health, Retrospective Studies, SARS-CoV-2, Travel},
