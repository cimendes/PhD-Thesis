@article{li_microbiome_2021,
	title = {Microbiome {Profiling} {Using} {Shotgun} {Metagenomic} {Sequencing} {Identified} {Unique} {Microorganisms} in {COVID}-19 {Patients} {With} {Altered} {Gut} {Microbiota}},
	volume = {12},
	issn = {1664-302X},
	url = {https://www.frontiersin.org/article/10.3389/fmicb.2021.712081},
	abstract = {COVID-19 is mainly associated with respiratory distress syndrome, but a subset of patients often present gastrointestinal (GI) symptoms. Imbalances of gut microbiota have been previously linked to respiratory virus infection. Understanding how the gut–lung axis affects the progression of COVID-19 can provide a novel framework for therapies and management. In this study, we examined the gut microbiota of patients with COVID-19 (n = 47) and compared it to healthy controls (n = 19). Using shotgun metagenomic sequencing, we have identified four microorganisms unique in COVID-19 patients, namely Streptococcus thermophilus, Bacteroides oleiciplenus, Fusobacterium ulcerans, and Prevotella bivia. The abundances of Bacteroides stercoris, B. vulgatus, B. massiliensis, Bifidobacterium longum, Streptococcus thermophilus, Lachnospiraceae bacterium 5163FAA, Prevotella bivia, Erysipelotrichaceae bacterium 6145, and Erysipelotrichaceae bacterium 2244A were enriched in COVID-19 patients, whereas the abundances of Clostridium nexile, Streptococcus salivarius, Coprococcus catus, Eubacterium hallii, Enterobacter aerogenes, and Adlercreutzia equolifaciens were decreased (p {\textless} 0.05). The relative abundance of butyrate-producing Roseburia inulinivorans is evidently depleted in COVID-19 patients, while the relative abundances of Paraprevotella sp. and the probiotic Streptococcus thermophilus were increased. We further identified 30 KEGG orthology (KO) modules overrepresented, with 7 increasing and 23 decreasing modules. Notably, 15 optimal microbial markers were identified using the random forest model to have strong diagnostic potential in distinguishing COVID-19. Based on Spearman’s correlation, eight species were associated with eight clinical indices. Moreover, the increased abundance of Bacteroidetes and decreased abundance of Firmicutes were also found across clinical types of COVID-19. Our findings suggest that the alterations of gut microbiota in patients with COVID-19 may influence disease severity. Our COVID-19 classifier, which was cross-regionally verified, provides a proof of concept that a set of microbial species markers can distinguish the presence of COVID-19.},
	urldate = {2022-02-28},
	journal = {Frontiers in Microbiology},
	author = {Li, Sijia and Yang, Siyuan and Zhou, Yuzheng and Disoma, Cyrollah and Dong, Zijun and Du, Ashuai and Zhang, Yongxing and Chen, Yong and Huang, Weiliang and Chen, Junru and Song, Deqiang and Chen, Zongpeng and Liu, Pinjia and Li, Shiqin and Zheng, Rong and Liu, Sixu and Razzaq, Aroona and Chen, Xuan and Tao, Siyi and Yu, Chengping and Feng, Tianxu and Liao, Wenyan and Peng, Yousong and Jiang, Taijiao and Huang, Jufang and Wu, Wei and Hu, Liqiang and Wang, Linghang and Li, Shanni and Xia, Zanxian},
	year = {2021},
}

@article{carleton_metagenomic_2019,
	title = {Metagenomic {Approaches} for {Public} {Health} {Surveillance} of {Foodborne} {Infections}: {Opportunities} and {Challenges}},
